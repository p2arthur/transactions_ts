{
  "compilerOptions": {
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "target": "ES6",
    "noImplicitAny": true,
    "strictNullChecks": false,
    "noImplicitReturns": false,
    "skipLibCheck": true,
    "removeComments": true,
    "experimentalDecorators": true
  },
  "include": ["src"]
}
